{
  "name": "medmaster", 
  // TODO: Change project name if deploying under a different name
  "version": "0.1.0",
  "private": true,
  "scripts": {
    // Starts the development server with hot reloading
    "dev": "next dev",
    // Creates a production build of the application
    "build": "next build",
    // Starts the production server
    "start": "next start",
    // Lints the code using ESLint
    "lint": "next lint",
    // Runs unit tests using Jest
    "test": "jest",
    // Placeholder script for database seeding. Refer to sql/seed.sql for manual seeding instructions in Supabase.
    "seed": "echo 'Seeding not implemented yet. Run SQL from sql/seed.sql in your Supabase dashboard.'",
    // Placeholder script to generate the daily quiz. To be implemented for cron job.
    "generate-daily-quiz": "echo 'Not implemented yet.'",
    // Formats code using Prettier
    "format": "prettier --write .",
    // Checks TypeScript types without emitting files
    "typecheck": "tsc --noEmit",
    // Starts the GenKit development server for AI flows
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    // Starts the GenKit development server with file watching
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts"
  },
  "dependencies": {
    // GenKit AI integration with Google AI models
    "@genkit-ai/googleai": "^1.14.1",
    // GenKit integration for Next.js
    "@genkit-ai/next": "^1.14.1",
    // Zod resolvers for React Hook Form
    "@hookform/resolvers": "^4.1.3",
    // Radix UI components
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@radix-ui/react-dialog": "^1.3.1",
    // Supabase JavaScript client library
    "@supabase/supabase-js": "^2.43.4",
    // Utility for conditional classes
    "class-variance-authority": "^0.7.1",
    // Utility for conditionally joining class names
    "clsx": "^2.1.1",
    // Date utility library
    "date-fns": "^3.6.0",
    // Loads environment variables from a .env file
    "dotenv": "^16.5.0",
    // React component for Embla Carousel
    "embla-carousel-react": "^8.6.0",
    // Firebase SDK (check if needed for this stage)
    "firebase": "^11.9.1",
    // Animation library
    "framer-motion": "^11.2.10",
    // GenKit core library
    "genkit": "^1.14.1",
    // Icon library
    "lucide-react": "^0.475.0",
    // Next.js framework
    "next": "15.3.3",
    // Utility to patch node modules
    "patch-package": "^8.0.0",
    // React library
    "react": "^18.3.1",
    // Date picker component
    "react-day-picker": "^8.10.1",
    // React DOM
    "react-dom": "^18.3.1",
    // Forms library for React
    "react-hook-form": "^7.54.2",
    // Charting library
    "recharts": "^2.15.1",
    // Utility for merging Tailwind CSS classes
    "tailwind-merge": "^3.0.1",
    // Tailwind CSS animation utilities
    "tailwindcss-animate": "^1.0.7",
    // Zod schema validation library
    "zod": "^3.24.2"
  },
  "devDependencies": {
    // React testing utilities
    "@testing-library/react": "^16.0.0",
    // TypeScript types for Jest
    "@types/jest": "^29.5.12",
    // TypeScript types for Node.js
    "@types/node": "^20",
    // TypeScript types for React
    "@types/react": "^18",
    // TypeScript types for React DOM
    "@types/react-dom": "^18",
    // PostCSS plugin to parse CSS and add vendor prefixes
    "autoprefixer": "^10.4.19",
    // ESLint configuration for Prettier
    "eslint-config-prettier": "^9.1.0",
    "genkit-cli": "^1.14.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "playwright": "^1.44.1",
    "postcss": "^8",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.1",
    // TypeScript language
    "typescript": "^5"
  }
}
